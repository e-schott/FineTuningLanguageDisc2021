  title = {The "Language Filter" Hypothesis: {{A}} Feasibility Study of Language Separation in Infancy Using Unsupervised Clustering of {{I}}-Vectors},
  shorttitle = {The \#{{x201C}};Language Filter \#{{x201D}}; Hypothesis},
  booktitle = {2016 {{Joint IEEE International Conference}} on {{Development}} and {{Learning}} and {{Epigenetic Robotics}} ({{ICDL}}-{{EpiRob}})},
