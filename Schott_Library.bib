@article{bergelsonEarlyWordComprehension2015,
  title = {Early {{Word Comprehension}} in {{Infants}}: {{Replication}} and {{Extension}}},
  shorttitle = {Early {{Word Comprehension}} in {{Infants}}},
  author = {Bergelson, Elika and Swingley, Daniel},
  year = {2015},
  month = oct,
  volume = {11},
