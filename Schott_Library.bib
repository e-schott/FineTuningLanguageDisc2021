  pages = {633--660},
  issn = {0169-0965},
  doi = {10.1080/01690960601000746},
  abstract = {During spoken word-recognition, bilinguals have been shown to access their two languages simultaneously. The present study examined effects of language proficiency and lexical status on parallel language activation. Language proficiency was manipulated by testing German-native and English-native bilingual speakers of German and English. Lexical status was manipulated by presenting target words that either overlapped in form across translation equivalents (cognate words) or did not overlap in form across translation equivalents (English-specific words). Participants identified targets (such as hen) from picture-displays that also included similar-sounding German competitor words (such as Hemd, “shirt”). Eye-movements to German competitors were used to index co-activation of German. Results showed that both bilingual groups co-activated German while processing cognate targets; however, only German-native bilinguals co-activated German while processing English-specific targets. These findings indicate that high language proficiency and cognate status boost parallel language activation in bilinguals.},
  file = {C\:\\Users\\Esther\\Zotero\\storage\\457WPDPP\\Blumenfeld and Marian - 2007 - Constraints on parallel activation in bilingual sp.pdf;C\:\\Users\\Esther\\Zotero\\storage\\WS67W34H\\Blumenfeld,Marian 2007.pdf},
  journal = {Language and Cognitive Processes},
  number = {5}
}

@article{blumenfeldCovertBilingualLanguage,
  title = {Covert {{Bilingual Language Activation}} through {{Cognate Word Processing}}: {{An Eye}}-Tracking {{Study}}},
  author = {Blumenfeld, Henrike K and Marian, Viorica},
  pages = {7},
  abstract = {The present study examined effects of cross-linguistic overlap and language proficiency on bilingual parallel language activation. Recognition of cognates (e.g., English hen, German Henne) was compared to recognition of non-cognates (e.g., English bike, German Fahrrad) in English-German bilinguals, German-English bilinguals, and monolinguals. In an eye-tracking paradigm, participants identified English cognate and non-cognate targets in picture displays that also contained German competitors with shared phonological onsets. The duration of phonological overlap between targets and competitors was manipulated. English-German bilinguals co-activated German competitors of cognate targets, while German-English bilinguals co-activated German competitors of non-cognate targets, as well as German competitors of cognate targets with high phonological overlap. These results suggest that proficiency in target and non-target languages, as well as cross-linguistic overlap, determine the extent of bilinguals’ parallel language activation.},
  file = {C\:\\Users\\Esther\\Zotero\\storage\\NKLPDYZ8\\Blumenfeld and Marian - Covert Bilingual Language Activation through Cogna.pdf},
